// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContactSubmission {
  id            String   @id @default(cuid())
  name          String
  email         String
  phone         String?
  restaurantName String?
  message       String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}